{	"manifest_version": 2,	"name": "Light Dial",	"version": "0.1",	"description": "Keep your favourite site",	"icons": {		"16": "icon16.png",		"32": "icon32.png",		"48": "icon48.png",		"128": "icon128.png"	},		"permissions": [		"bookmarks",		"tabs",		"history",		"chrome://favicon/",		"http://*/",		"https://*/",		"contextMenus"	],	"chrome_url_overrides": {		"newtab": "newtab.html"		},		"browser_action": {		"default_title": "Light Dial",		"default_icon": "icon48.png",		"default_popup": "popup.html"	},		"options_page": "options.html"}